
<h1>{L_TITLE}</h1>

<p>{L_TITLE_EXPLAIN}</p>

<table width="100%" cellpadding="4" cellspacing="1" border="0" class="forumline">
	<!-- BEGIN message -->
	<tr>
		<td class="errorline" align="center" colspan="5">
			{message.text}
		</td>
	</tr>
	<!-- END message -->
  <tr> 
	<th nowrap="nowrap" height="25" class="thCornerL">{L_ACTION}</th>
	<th height="25" class="thCornerR">{L_DESCRIPTION}</th>
  </tr>
	<!-- BEGIN menuitems -->
  <tr> 
	<td class="{menuitems.color}" nowrap="nowrap">{menuitems.action}</td>
	<td class="{menuitems.color}">{menuitems.description}</td>
  </tr>
	<!-- END menuitems -->
</table>
<br />
